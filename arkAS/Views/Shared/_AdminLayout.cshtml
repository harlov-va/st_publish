@using arkAS.Models;
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@(ViewBag.Title ?? "Ark AS")</title>
    <link href="/js/jquery-1.11.2.min.js" rel="stylesheet">
    <link href="/js/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="/js/bootstrap/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="/js/as/as.css" rel="stylesheet">
    <link href="~/content/Site.css" rel="stylesheet">
    <link href="/js/AS/Crud/as.crud.css" rel="stylesheet">
    <script src="/js/jquery-1.11.2.min.js"></script>
    <script src="/js/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/js/as/as.js" charset="windows-1251"></script>
    <script src="~/js/as/as.sys.js" charset="windows-1251"></script>
    <script src="~/js/as/as.tools.js" charset="windows-1251"></script>
    <script src="~/js/as/as.makeup.js" charset="windows-1251"></script>
    <script src="~/js/as/as.ark.js" charset="windows-1251"></script>
    <script src="~/js/as/crud/as.crud.js" charset="windows-1251"></script>
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/themes/smoothness/jquery-ui.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/jquery-ui.min.js?modules=core,widget,mouse,sortable"></script>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Ark AS", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("О нас", "About", "Home")</li>
                    <li>@Html.ActionLink("Как связаться", "Contact", "Home")</li>
                   
                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        </div>
    </div>
    <div class="container body-content">
        <div class="body-content2">

            @RenderBody()
        </div>
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Система разработки CRM (личных кабинетов) на заказ Ark AS (Web-automation.ru)</p>
        </footer>
    </div>
    
</body>
</html>
